{
  "include": [
    "src/**/*",
    "@types/*.d.ts"
, "src/jobs/.ts"  ],
  "compilerOptions": {
    "target": "es2020",
    "module": "CommonJS",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "rootDir": "./",
    "outDir": "./dist",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}